java.util.List<String[]> data = new java.util.LinkedList<String[]>();
for (int i = 1; i < 10; i++) 
	data.add(new String[]{"entry1", "value1", "value2", "value3"});

System.out.println(data);
String[][] newdata = data.toArray(new String[0][0]);
for(int i=0; i< data.size(); i++){
	for(String s : newdata[i])
		System.out.print(s);
	System.out.println();
}